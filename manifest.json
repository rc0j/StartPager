{
  "manifest_version": 2,
  "name": "Start Pager",
  "short_name": "startpager",
  "version": "7.81.2",
  "description": "Free, open-source new tab focused on being zen.",
  "author": "Raif Coonjah (rc0j)",

  "icons": {
    "48": "/assets/img/icon48.png",
    "96": "/assets/img/icon96.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "chrome_settings_overrides": {
    "homepage": "index.html"
  },

  "applications": {
    "gecko": {
      "id": "{aa7b057d-721f-4ed8-ae73-563f767006ee}",
      "update_url": "https://raw.githubusercontent.com/rc0j/Saturn-startpage/master/updates_v8.json"
    }
  },

  "permissions": ["storage"]
}
